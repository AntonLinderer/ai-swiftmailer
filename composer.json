{
	"name": "aimeos/ai-swiftmailer",
	"description": "Aimeos SwiftMailer adapter",
	"keywords": ["aimeos", "adapter", "swiftmailer", "mail"],
	"homepage": "http://aimeos.org",
	"type": "aimeos-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/ai-swiftmailer",
		"issues": "https://github.com/aimeos/ai-swiftmailer/issues",
		"forum": "https://help.aimeos.org",
		"wiki": "https://docs.aimeos.org"
	},
	"require": {
		"swiftmailer/swiftmailer": ">=4.0"
	},
	"autoload": {
		"psr-0": {
			"MW": "lib/custom/src"
		},
		"classmap": [
			"lib/custom/src"
		]
	}
}
